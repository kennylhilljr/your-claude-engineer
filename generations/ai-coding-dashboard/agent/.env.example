# Environment Configuration
ENVIRONMENT=development

# Server Configuration
BACKEND_PORT=8000
BACKEND_HOST=0.0.0.0
FRONTEND_PORT=3010

# AI Provider API Keys
# OpenRouter API Key (required for KAN-51)
OPENROUTER_API_KEY=your-openrouter-api-key-here
OPENROUTER_MODEL=anthropic/claude-sonnet-4.5

# Agent Configuration
AGENT_TEMPERATURE=0.7
AGENT_MAX_TOKENS=4096

# Other AI Provider Keys (optional)
# OPENAI_API_KEY=sk-your-openai-key-here
# ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here

# Database Configuration (for later use)
# DATABASE_URL=postgresql://user:password@localhost:5432/ai_dashboard

# Redis Configuration (for later use)
# REDIS_URL=redis://localhost:6379/0

# Logging
LOG_LEVEL=info

# Debug Mode
DEBUG=true
